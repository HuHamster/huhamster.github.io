    <!-- JavaScript for toggling the mobile menu and the scroll-to-top button -->
    <script>
        // Function to toggle the 'active' class of the mobile navigation menu
        function toggleMenu() {
            const mobileMenu = document.getElementById('mobile-menu');
            mobileMenu.classList.toggle('active'); // Add or remove the 'active' class
        }

        // Close the mobile menu when any link inside it is clicked
        document.querySelectorAll('#mobile-menu a').forEach(item => {
            item.addEventListener('click', () => {
                const mobileMenu = document.getElementById('mobile-menu');
                // Check if the mobile menu currently has the 'active' class
                if (mobileMenu.classList.contains('active')) {
                    // If so, remove the 'active' class to hide the menu
                    mobileMenu.classList.remove('active');
                }
            });
        });

        // Get the scroll-to-top button element
        const scrollToTopBtn = document.getElementById("scrollToTopBtn");

        // Add a scroll event listener to the window
        window.onscroll = function() {
            // Check if the user has scrolled more than 20 pixels from the top
            if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
                // If scrolled, display the button and make it visible with a transition
                scrollToTopBtn.style.display = "block";
                scrollToTopBtn.style.opacity = "1";
                scrollToTopBtn.style.transform = "translateY(0)";
            } else {
                // If at the top, start the fade-out and hide transition
                scrollToTopBtn.style.opacity = "0";
                scrollToTopBtn.style.transform = "translateY(10px)";
                // After the transition completes (300ms), truly hide the button
                // This prevents it from taking up space while invisible
                setTimeout(() => {
                    if (scrollToTopBtn.style.opacity === "0") {
                        scrollToTopBtn.style.display = "none";
                    }
                }, 300);
            }
        };

        // Add a click event listener to the scroll to top button
        scrollToTopBtn.addEventListener("click", function() {
            // Scroll the window to the top with a smooth animation
            window.scrollTo({
                top: 0,
                behavior: "smooth"
            });
        });

        // Push AdSense ads after the DOM is fully loaded
        document.addEventListener('DOMContentLoaded', function() {
            // Push the in-article ad
            // adsbygoogle is guaranteed to be defined here due to the script tag in the head
            (adsbygoogle = window.adsbygoogle || []).push({});
            // Push the fluid ad
            (adsbygoogle = window.adsbygoogle || []).push({});
        });
    </script>
</body>
